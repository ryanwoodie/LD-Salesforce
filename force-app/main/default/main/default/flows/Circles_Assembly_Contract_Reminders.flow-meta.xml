    <formulas>
        <name>contact_and_or_planning_call</name>
        <dataType>String</dataType>
        <expression>IF(ISPICKVAL({!$Record.StageName},&quot;Booking Confirmed / Contract sent&quot;), 
   &apos;&lt;li&gt;&lt;strong&gt;Fill out the School Contract&lt;/strong&gt; &lt;a href=&quot;&apos; &amp; SUBSTITUTE(SUBSTITUTE({!$Record.School_Contract_Form__c}, &apos;_HL_ENCODED_&apos;, &apos;&apos;), &apos;_HL_&apos;, &apos;&apos;) &amp; &apos;&quot;&gt; &gt;&gt;HERE&lt;&lt; &lt;/a&gt; When completed this will generate an invoice.&lt;/li&gt;&apos;, 
   &apos;&apos;) &amp; 
IF(NOT({!$Record.Planning_call_complete__c}), 
   &apos;&lt;li&gt;&lt;strong&gt;Book a Planning Meeting&lt;/strong&gt; with me in my calendar to discuss all the details. Click &lt;a href=&quot;&apos; &amp; {!$Record.Owner.Calendar_URL__c} &amp; &apos;&quot; target=&quot;_blank&quot; rel=&quot;noopener&quot;&gt; &gt;&gt;HERE&lt;&lt; &lt;/a&gt; to choose a time that works for you!&lt;/li&gt;&apos;, 
   &apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>invoice_date_more_than_2weeks</name>
        <dataType>Boolean</dataType>
        <expression>{!$Record.Invoice_Date__c} + 14 &gt; {!date_sent_contract}</expression>
    </formulas>
    <formulas>
        <name>one_week_after_contract_send</name>
        <dataType>Boolean</dataType>
        <expression>IF({!$Flow.CurrentDate} - {!date_sent_contract} &gt; 6, TRUE, FALSE)</expression>
    </formulas>
    <formulas>
        <name>subject_line</name>
        <dataType>String</dataType>
        <expression>&quot;LiveDifferent &quot; &amp; 
IF(
    ISPICKVAL({!$Record.StageName},&quot;Booking Confirmed / Contract sent&quot;) &amp;&amp; 
    {!$Record.Planning_call_complete__c}, 
    &quot;contract reminder&quot;, 
    IF(
        ISPICKVAL({!$Record.StageName},&quot;Booking Confirmed / Contract sent&quot;) &amp;&amp; 
        NOT({!$Record.Planning_call_complete__c}), 
        &quot;contract &amp; planning call reminder&quot;, 
        IF(
            NOT({!$Record.Planning_call_complete__c}), 
            &quot;planning call reminder&quot;, 
            &quot;&quot;
        )
    )
)</expression>
    </formulas>
    <formulas>
        <name>two_weeks_after_contact_send</name>
        <dataType>Boolean</dataType>
        <expression>IF({!$Flow.CurrentDate} - {!date_sent_contract} &gt; 14, TRUE, FALSE)</expression>
    </formulas>
    <formulas>
        <name>x2_mins_after_flow_trigger</name>
        <dataType>Boolean</dataType>
        <expression>if({!$Flow.CurrentDateTime}-{!$Record.Flow_trigger_date_time__c} &gt;( 2/1440),TRUE,FALSE)</expression>
    </formulas>
    <formulas>
        <name>x3_5monthsgreater</name>
        <dataType>Boolean</dataType>
        <expression>{!$Flow.CurrentDate} + 105 &gt; {!$Record.Invoice_Date__c}</expression>
    </formulas>
