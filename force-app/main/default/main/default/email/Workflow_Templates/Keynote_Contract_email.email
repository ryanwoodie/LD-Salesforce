						    <span style="text-decoration: none; color: #848484; font-weight: normal;"> {!$Organization.City}, {!$Organization.State}   {!$Organization.PostalCode}</span><br/>
						    <span style="text-decoration: none; color: #848484; font-weight: normal;">1 (866) 432 4464</span><br/>
                                                    <a href="mailto:{!NullValue(Opportunity.OwnerEmail, "hh@livedifferent.com")}" style="text-decoration: none; color: #848484; font-weight: normal;">{!NullValue(Opportunity.OwnerEmail, "bookings@livedifferent.com")}</a>

